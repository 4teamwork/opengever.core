<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="plone.app.dexterity">
<body>

<metal:main fill-slot="content-core">
  <metal:content-core define-macro="content-core">

    <div i18n:domain="opengever.meeting" id="opengever_meeting_meetingtemplate"
         tal:define="paragraphs view/paragraphs"
         tal:attributes="data-update-order-url view/url_update_order">

      <ul class="paragraphs-list">
        <li tal:repeat="paragraph paragraphs"
            tal:content="paragraph/title"
            tal:attributes="data-uid paragraph/id"></li>
      </ul>

    </div>

  </metal:content-core>
</metal:main>

</body>
</html>
