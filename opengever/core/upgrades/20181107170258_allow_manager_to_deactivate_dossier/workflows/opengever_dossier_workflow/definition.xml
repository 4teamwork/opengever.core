    <guard>
      <guard-permission>Review portal content</guard-permission>
    </guard>
  </transition>
  <transition
      transition_id="dossier-transition-offer"
      title="Offers dossier"
      new_state="dossier-state-offered"
      trigger="USER"
      before_script=""
      after_script="">
