<object name="mimetypes_registry" meta_type="MimeTypes Registry">

  <!-- Common file formats -->
  <mimetype
      mimetypes="image/gif"
      icon_path="icon_dokument_bilddatei.gif"
      />

  <mimetype
      mimetypes="image/jpeg
                 image/pjpeg"
      icon_path="icon_dokument_bilddatei.gif"
      />

  <mimetype
      mimetypes="image/png"
      icon_path="icon_dokument_bilddatei.gif"
      />

  <mimetype
      mimetypes="image/bmp
                 image/x-bmp
                 image/x-MS-bmp"
      icon_path="icon_dokument_bilddatei.gif"
      extensions="bmp"
      />

  <mimetype
      mimetypes="message/rfc822"
      icon_path="icon_dokument_email.gif"
      />

  <mimetype
      mimetypes="text/plain"
      icon_path="icon_dokument_text.gif"
      extensions="txt diff conf def in text pot ksh list log asc ini"
      />

  <mimetype
      mimetypes="text/html"
      icon_path="icon_dokument_webdokument.gif"
      />

  <mimetype
      name="Rich Text Format (RTF)"
      mimetypes="application/rtf"
      binary="True"
      extensions="rtf"
      globs="*.rtf"
      icon_path="icon_dokument_text.gif"
      />

  <mimetype
      mimetypes="application/zip
                 application/x-zip-compressed"
      icon_path="icon_dokument_zip.gif"
      />

  <mimetype
      mimetypes="application/pdf
                 application/x-pdf
                 image/pdf"
      icon_path="icon_dokument_pdf.gif"
      />

  <!-- Remove .ai from postscript types, we want it to be associated only with Adobe Illustrator -->
  <mimetype
      name="PS document"
      binary="True"
      extensions="ps eps"
      globs="*.ps"
      icon_path="ps.png"
      mimetypes="application/postscript"
      />

  <!-- Microsoft Office Formats -->

  <!-- MS Outlook -->
  <mimetype
      name="Microsoft Outlook Message"
      binary="True"
      extensions="msg"
      globs="*.msg"
      icon_path="icon_dokument_email.gif"
      mimetypes="application/vnd.ms-outlook"
      />

  <!-- MS OneNoe -->
  <mimetype
      name="Microsoft OneNote"
      binary="True"
      extensions="one"
      globs="*.one"
      icon_path="icon_dokument_onenote.gif"
      mimetypes="application/onenote"
      />

  <!-- MS Excel -->
  <mimetype
      name="Microsoft Excel Spreadsheet"
      binary="True"
      extensions="xls xlt xla xlb xlc xld xll xlm xlw"
      globs="*.xls *.xlt *.xla *.xlb *.xlc *.xld *.xll *.xlm *.xlw"
      icon_path="icon_dokument_excel.gif"
      mimetypes="application/vnd.ms-excel
                 application/msexcel
                 application/x-msexcel"
      />

  <mimetype
      name="Microsoft Excel Spreadsheet (OOXML)"
      binary="True"
      extensions="xlsx"
      globs="*.xlsx"
      icon_path="icon_dokument_excel.gif"
      mimetypes="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"
      />

  <mimetype
      name="Microsoft Excel Template (OOXML)"
      binary="True"
      extensions="xltx"
      globs="*.xltx"
      icon_path="icon_dokument_excel.gif"
      mimetypes="application/vnd.openxmlformats-officedocument.spreadsheetml.template"
      />

  <mimetype
      name="Microsoft Excel Spreadsheet (Macro Enabled)"
      binary="True"
      extensions="xlsm"
      globs="*.xlsm"
      icon_path="icon_dokument_excel.gif"
      mimetypes="application/vnd.ms-excel.sheet.macroEnabled.12"
