<?xml version="1.0"?>
<object name="mimetypes_registry" meta_type="MimeTypes Registry">

  <!-- Common file formats -->
  <mimetype mimetypes="image/gif"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/bilddatei.gif" />

  <mimetype mimetypes="image/jpeg
                       image/pjpeg"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/bilddatei.gif" />

  <mimetype mimetypes="image/png"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/bilddatei.gif" />

  <mimetype mimetypes="image/bmp
                       image/x-bmp
                       image/x-MS-bmp"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/bilddatei.gif" />

  <mimetype mimetypes="message/rfc822"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/email.gif" />

  <mimetype mimetypes="text/plain"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/text.gif" />

  <mimetype mimetypes="text/html"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/webdokument.gif" />

  <mimetype name="Rich Text Format (RTF)"
            mimetypes="application/rtf"
            binary="True"
            extensions="rtf"
            globs="*.rtf"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/text.gif" />

  <mimetype mimetypes="application/zip
                       application/x-zip-compressed"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/zip.gif" />

  <mimetype mimetypes="application/pdf
                       application/x-pdf
                       image/pdf"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/pdf.gif" />

  <!-- Remove .ai from postscript types, we want it to be associated only with Adobe Illustrator -->
  <mimetype name="PS document"
            binary="True"
            extensions="ps eps"
            globs="*.ps"
            icon_path="ps.png"
            mimetypes="application/postscript" />

  <!-- Microsoft Office Formats -->

  <!-- MS Excel -->
  <mimetype name="Microsoft Excel Spreadsheet"
            binary="True"
            extensions="xls xlt xla xlb xlc xld xll xlm xlw"
            globs="*.xls *.xlt *.xla *.xlb *.xlc *.xld *.xll *.xlm *.xlw"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/excel.gif"
            mimetypes="application/vnd.ms-excel
                       application/msexcel
                       application/x-msexcel" />

  <mimetype name="Microsoft Excel Spreadsheet (OOXML)"
            binary="True"
            extensions="xlsx"
            globs="*.xlsx"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/excel.gif"
            mimetypes="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" />

  <mimetype name="Microsoft Excel Template (OOXML)"
            binary="True"
            extensions="xltx"
            globs="*.xltx"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/excel.gif"
            mimetypes="application/vnd.openxmlformats-officedocument.spreadsheetml.template" />

  <!-- MS Word -->
  <mimetype name="Microsoft Word Document"
            binary="True"
            extensions="doc wiz dot"
            globs="*.doc *.wiz *.dot"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/word.gif"
            mimetypes="application/msword" />

  <mimetype name="Microsoft Word Document (OOXML)"
            binary="True"
            extensions="docx"
            globs="*.docx"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/word.gif"
            mimetypes="application/vnd.openxmlformats-officedocument.wordprocessingml.document" />

  <mimetype name="Microsoft Word Template (OOXML)"
            binary="True"
            extensions="dotx"
            globs="*.dotx"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/word.gif"
            mimetypes="application/vnd.openxmlformats-officedocument.wordprocessingml.template" />


  <!-- MS Powerpoint -->
  <mimetype name="Microsoft PowerPoint Presentation"
            binary="True"
            extensions="ppt pot pps ppa pwz ppz"
            globs="*.ppt *.pot *.pps *.ppa *.pwz *.ppz"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/powerpoint.gif"
            mimetypes="application/vnd.ms-powerpoint
                       application/powerpoint
                       application/mspowerpoint
                       application/x-mspowerpoint"/>

  <mimetype name="Microsoft PowerPoint Presentation (OOXML)"
            binary="True"
            extensions="pptx"
            globs="*.pptx"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/powerpoint.gif"
            mimetypes="application/vnd.openxmlformats-officedocument.presentationml.presentation" />

  <mimetype name="Microsoft PowerPoint Template (OOXML)"
            binary="True"
            extensions="potx"
            globs="*.potx"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/powerpoint.gif"
            mimetypes="application/vnd.openxmlformats-officedocument.presentationml.template" />

  <!-- MS Visio -->
  <mimetype name="Microsoft Visio document"
            binary="True"
            extensions="vsd vss vst vsw vdx vsx vtx vsl"
            globs="*.vsd *.vss *.vst *.vsw *.vdx *.vsx *.vtx *.vsl"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/visio.gif"
            mimetypes="application/vnd.visio"/>

  <!-- MS Project -->
  <mimetype name="MS Project File"
            binary="True"
            extensions="mpp"
            globs="*.mpp"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/msproject.gif"
            mimetypes="application/vnd.ms-project"/>

  <mimetype name="MS Project File (generic)"
            binary="True"
            extensions="mpt mpv mpx"
            globs="*.mpt *.mpv *.mpx"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/msproject.gif"
            mimetypes="application/x-project"/>


  <!-- Adobe Formats -->

  <mimetype name="Adobe Indesign Document"
            binary="True"
            extensions="indd ind"
            globs="*.indd *.ind"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/indesign.gif"
            mimetypes="application/x-indesign"/>

  <mimetype name="Adobe Photoshop Document"
            binary="True"
            extensions="psd"
            globs="*.psd"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/photoshop.gif"
            mimetypes="image/vnd.adobe.photoshop"/>

  <mimetype name="Adobe Illustrator Document"
            binary="True"
            extensions="ai"
            globs="*.ai"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/illustrator.gif"
            mimetypes="application/illustrator"/>

  <!-- OpenOffice 1.0 Formats -->

  <mimetype name="OpenOffice Calc spreadsheet"
            binary="True"
            extensions="sxc"
            globs="*.sxc"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_calc.gif"
            mimetypes="application/vnd.sun.xml.calc"/>

  <mimetype name="OpenOffice Calc template"
            binary="True"
            extensions="stc"
            globs="*.stc"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_calc.gif"
            mimetypes="application/vnd.sun.xml.calc.template"/>

  <mimetype name="OpenOffice Draw drawing"
            binary="True"
            extensions="sxd"
            globs="*.sxd"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_draw.gif"
            mimetypes="application/vnd.sun.xml.draw"/>

  <mimetype name="OpenOffice Draw template"
            binary="True"
            extensions="std"
            globs="*.std"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_draw.gif"
            mimetypes="application/vnd.sun.xml.draw.template"/>

  <mimetype name="OpenOffice Impress presentation"
            binary="True"
            extensions="sxi"
            globs="*.sxi"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_impress.gif"
            mimetypes="application/vnd.sun.xml.impress"/>

  <mimetype name="OpenOffice Impress template"
            binary="True"
            extensions="sti"
            globs="*.sti"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_impress.gif"
            mimetypes="application/vnd.sun.xml.impress.template"/>

  <mimetype name="OpenOffice Math formula"
            binary="True"
            extensions="sxm"
            globs="*.sxm"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_math.gif"
            mimetypes="application/vnd.sun.xml.math"/>

  <mimetype name="OpenOffice Writer document"
            binary="True"
            extensions="sxw"
            globs="*.sxw"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_writer.gif"
            mimetypes="application/vnd.sun.xml.writer"/>

  <mimetype name="OpenOffice Writer global document"
            binary="True"
            extensions="sxg"
            globs="*.sxg"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_writer.gif"
            mimetypes="application/vnd.sun.xml.writer.global"/>

  <mimetype name="OpenOffice Writer template"
            binary="True"
            extensions="stw"
            globs="*.stw"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_writer.gif"
            mimetypes="application/vnd.sun.xml.writer.template"/>


  <!-- ODF (OpenDocument Format) -->

  <mimetype name="ODT document"
            binary="True"
            extensions="odt"
            globs="*.odt"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_writer.gif"
            mimetypes="application/vnd.oasis.opendocument.text"/>

  <mimetype name="ODT template"
            binary="True"
            extensions="ott"
            globs="*.ott"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_writer.gif"
            mimetypes="application/vnd.oasis.opendocument.text-template"/>

  <mimetype name="ODM document"
            binary="True"
            extensions="odm"
            globs="*.odm"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_writer.gif"
            mimetypes="application/vnd.oasis.opendocument.text-master"/>

  <mimetype name="OTH template"
            binary="True"
            extensions="oth"
            globs="*.oth"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_writer.gif"
            mimetypes="application/vnd.oasis.opendocument.text-web"/>

  <mimetype name="ODS spreadsheet"
            binary="True"
            extensions="ods"
            globs="*.ods"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_calc.gif"
            mimetypes="application/vnd.oasis.opendocument.spreadsheet"/>

  <mimetype name="ODS template"
            binary="True"
            extensions="ots"
            globs="*.ots"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_calc.gif"
            mimetypes="application/vnd.oasis.opendocument.spreadsheet-template"/>

  <mimetype name="ODP presentation"
            binary="True"
            extensions="odp"
            globs="*.odp"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_impress.gif"
            mimetypes="application/vnd.oasis.opendocument.presentation"/>

  <mimetype name="ODP template"
            binary="True"
            extensions="otp"
            globs="*.otp"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_impress.gif"
            mimetypes="application/vnd.oasis.opendocument.presentation-template"/>

  <mimetype name="ODG drawing"
            binary="True"
            extensions="odg"
            globs="*.odg"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_draw.gif"
            mimetypes="application/vnd.oasis.opendocument.graphics"/>

  <mimetype name="ODG template"
            binary="True"
            extensions="otg"
            globs="*.otg"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_draw.gif"
            mimetypes="application/vnd.oasis.opendocument.graphics-template"/>

  <mimetype name="ODF formula"
            binary="True"
            extensions="odf"
            globs="*.odf"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/openoffice_math.gif"
            mimetypes="application/vnd.oasis.opendocument.formula"/>

  <!-- Various other file formats -->

  <mimetype name="Mindjet MindManager File"
            binary="True"
            extensions="mmap"
            globs="*.mmap"
            icon_path="++theme++plonetheme.teamraum/images/mimetypes/mindmanager.gif"
            mimetypes="application/vnd.mindjet.mindmanager"/>
</object>
