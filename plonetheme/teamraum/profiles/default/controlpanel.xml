<?xml version="1.0"?>
<object name="portal_controlpanel"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n"
        i18n:domain="plonetheme.teamraum"
        purge="False">

  <configlet
      title="Teamraum theme"
      action_id="plonetheme.teamraum"
